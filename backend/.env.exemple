# OpenAI API key
# Used for LLM features (chat, embeddings, agents, etc.)
# Get it from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-openai-api-key


# Opik API credentials (LLM observability / tracing)
# Get these from your Opik dashboard
OPIK_API_KEY=your-opik-api-key
OPIK_WORKSPACE=your-opik-workspace-name


# YouTube Data API key
# Enable "YouTube Data API v3" in Google Cloud Console
YOUTUBE_API_KEY=your-youtube-api-key

# Google Places API key
# Enable "Places API" in Google Cloud Console
GOOGLE_PLACES_API_KEY=your-google-places-api-key


# Supabase project URL (server-side)
# From Supabase Dashboard → Project Settings → API
SUPABASE_URL=https://your-project-id.supabase.co

# Supabase service role key (FULL ACCESS — server-side only)
# ⚠️ Never expose this key to the frontend
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key


# Allowed CORS origins (comma-separated)
# Controls which domains can access your backend
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com
